
<%- include('header') -%>
<body>

       <div class="box" id="heading">

              <h1>It's a <%= ListTitle %></h1>
       </div>
       <div class="box">

              <% for (var i=0;i < newListItems.length ;i++) { %>
              <div class="item">
<input type="checkbox">
                     <p><%=newListItems[i] %></p>
              </div>
              <% } %>

              <form class="item " action="/" method="post">
                     <input type="text" name="todo" placeholder="New Item" autocomplete="off">
                     <button type="submit" name="list" name="list" value=<%=ListTitle%>>+</button>
                     
              </form>
              
       </div>
</body>
<%- include('footer') -%>
